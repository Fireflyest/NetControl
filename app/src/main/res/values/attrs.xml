<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LightDish">
        <attr name="showText" format="boolean" />
        <attr name="labelPosition" format="enum">
            <enum name="canter" value="0"/>
            <enum name="top" value="1"/>
            <enum name="bottom" value="2"/>
        </attr>
        <attr name="strokeWidth" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ColorfulBar">
        <attr name="x0" format="dimension" />
        <attr name="y0" format="dimension" />
        <attr name="x1" format="dimension" />
        <attr name="y1" format="dimension" />
        <attr name="joinWidth" format="dimension" />
        <attr name="progressColor" format="color" />
    </declare-styleable>
</resources>